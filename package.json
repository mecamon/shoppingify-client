{
  "name": "shoppingify-client",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "jest --watchAll",
    "preview": "vite preview",
    "json:server": "json-server --watch src/services/db.json --port 3004"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "i18next": "^21.6.16",
    "json-server": "^0.17.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-i18next": "^11.16.7",
    "react-router-dom": "6",
    "vite-plugin-environment": "^1.1.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "27.0.0",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "vite": "^2.9.5"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|webp|avif|svg|module.css)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css | scss | sass)$": "identity-obj-proxy"
    }
  }
}
